<html>

<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>APR Project</title>
	<link rel="stylesheet" href="/assets/aprstyle.css" type="text/css" charset="utf-8" />
	<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
	
	<!--[if lte IE 7]>
		<link rel="stylesheet" href="css/ie.css" type="text/css" charset="utf-8" />	
	<![endif]-->
</head>


<body>

<div id="login_box">
<% if user_signed_in? %>
	Signed in as <%= current_user.username %>. Not you?
	<%= link_to "Sign out", destroy_user_session_path %> 
<% end %>

</div>

<% flash.each do |name, msg| %>
	<%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>


<div id="wrapper">

		
		<div id="head">
		
			<div id="banner">
				<img src="/assets/banner.jpg" />
		
			</div>
			<div id="navigation">
			
				
				<ul>
					<li><a href="/">Home </a></li>
					<li><a href="http://www.unm.edu/"> UNM Main Page</a></li>
					<li><a href="http://provost.unm.edu/">UNM Provost Office</a> </li>
					
				
				</ul>
				<br  class="clear"/>
			
			</div>
			
					<div>

						<%= yield %>

					</div>
		
		</div>

		<div id="content">
		</div>

		<div id="footer">

		</div>


	<p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>


</div>




</body>
</html>